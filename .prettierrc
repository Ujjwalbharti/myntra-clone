{
   "plugins": ["prettier-plugin-tailwindcss"],
   "tailwindConfig": "./tailwind.config.ts",
   "semi": true,
   "singleQuote": true,
   "trailingComma": "es5",
   "printWidth": 100,
   "tabWidth": 3,
   "useTabs": false,
   "jsxSingleQuote": false,
   "bracketSpacing": true,
   "arrowParens": "always",
   "endOfLine": "lf",
   "jsxBracketSameLine": false
}
